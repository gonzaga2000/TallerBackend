model Aircrafts {
  id               Int               @id @default(autoincrement())
  aircraftID String            
  name String            
  aircraftType String            
}

model Airports {
  id               Int               @id @default(autoincrement())
  airportIATA String            
  name String            
  city String            
  country String            
